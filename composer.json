{
    "name": "bnf/zend-diactoros-service-provider",
    "description": "container-interop/service-provider compatible service provider for zend-diactoros",
    "keywords": [
        "psr-11",
        "service-provider"
    ],
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "benjaminfranzke@gmail.com",
            "role": "Developer",
            "homepage": "https://bnf.dev"
        }
    ],
    "license": "GPL-2.0+",
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0",
        "psr/http-factory": "^1.0",
        "container-interop/service-provider": "~0.4.0",
        "zendframework/zend-diactoros": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bnf\\ZendDiactoros\\": "src"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-strict-rules": "^0.11.0",
        "squizlabs/php_codesniffer": "^3.4",
        "bnf/di": "^0.1.3",
        "slim/psr7": "^0.4.0"
    }
}
